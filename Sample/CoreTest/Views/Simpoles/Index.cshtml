@model SimpolesViewModels
@{
    ViewData["Title"] = "内容列表";
}

<div class="panel panel-default todo-panle">
    <div class="panel-heading">@ViewData["Title"] </div>
    <table class="table table-hover">
        <thead>
            <tr>
                <td><input type="checkbox" class="done-checkbox" /></td>
                <td>ID</td>
                <td>名称</td>
                <td>内容</td>
                <td>创建人</td>
                <td>创建时间</td>
            </tr>

        </thead>
       @foreach (var item in Model.simpoles)
       {
        <tr>
            <td>
                <input type="checkbox" class="done-checkbox" />
            </td>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Context</td>
            <td>@item.CreatBy</td>
            <td>@item.CreateDate</td>
        </tr>
       }
    </table>foreach

</div>

